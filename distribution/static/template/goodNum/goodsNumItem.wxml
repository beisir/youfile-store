<template name="goodsItem">
  <view class='template'>
    <view class='goods-item com-flex-between'>
      <image class='goods-icon' src='/image/poster-right-in.png'></image>
      <view class='flexone' style='overflow:hidden;'>
        <view class='com-flex-center font28 com-blod-font'>
          <text class='warning-tip font24'>预警</text>
          <text class='flexone com-text-ellipsis'>{{name}}</text>
        </view>
        <view class='font24 com-gray-color' style='padding:10rpx 0'>货号：23123123</view>
        <view class='font26'>
          <text>库存：</text>
          <text class='com-red-color'>2</text>
          <text class='com-yellow-color'>3</text>
        </view>
      </view>
      <image class='com-right-icon' src='/image/poster-right-in.png' style='margin-top:60rpx;'></image>
    </view>
  </view>
</template>