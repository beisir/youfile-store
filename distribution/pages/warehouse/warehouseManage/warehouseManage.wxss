/* distribution/pages/warehouse/warehouseManage/warehouseManage.wxss */
@import "/distribution/static/wxss/common";

.com-padding-box{padding: 20rpx 20rpx 0;}
/* 搜索 */
.search-input-box{border-radius:10rpx;background: #fff;height: 100rpx;flex: 1;font-size: 28rpx;}
.search-img{width: 30rpx;height: 30rpx;}
.search-btn{width: 120rpx;padding: 0;}
/* 仓库 */
.more-icon{width: 7rpx;height: 45rpx;padding: 0 20rpx;}
.house-name{font-size: 36rpx;font-weight: 600;padding: 0 20rpx;}
.house-tip{width: 80rpx;height: 30rpx;line-height:30rpx;text-align:center;font-size: 24rpx;border-radius:5rpx;border:1rpx solid #f8b616;color: #f8b616;}
.house-item{background: #fff;border-radius: 10rpx;padding: 30rpx;margin-bottom: 20rpx;}
.house-des-font{font-size: 26rpx;color: #999;padding: 10rpx 0;line-height: 1;}
.bottom-single-btn{background: #f8b616}