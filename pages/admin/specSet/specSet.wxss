.sp-cont{color:#333;padding:20rpx;background:#fff;border-radius:8rpx;margin:20rpx;}
.sp-color image{width:40rpx;height:40rpx;float: right;margin-left: 15rpx;margin-top: 6rpx}
page{height:99%}
.sp-color image.img{right:70rpx}
.spec-c{padding-bottom:150rpx}
.sp-color{font-size:34rpx;font-weight:600;margin-bottom:30rpx;padding:0 10rpx;position:relative}
.sp-button{margin-bottom:20rpx;margin-top:70rpx}
.sp-button view{display:inline-block;background:#F1F1F1;height:70rpx;line-height:70rpx;position:relative;font-size:26rpx;border-radius:10rpx;margin:10rpx;position:relative;text-align:center;min-width: 198rpx}
.sp-button view text{position:absolute;background:rgba(0,0,0,0.6);width:190rpx;top:-84rpx;left:50%;margin-left:-95rpx;border-radius:10rpx;z-index:10;font-size:24rpx;color:#fff}
.addTemp{height: 100rpx;width: 120rpx;position: absolute;top: 0;right: 0;background: rgba(255,255,255,0.9);text-align: center;}
.addTemp image{width: 60rpx;height: 60rpx;font-size: 24rpx;}
.addTemp view{line-height: 10rpx}
.sp-button view image{width:30rpx;height:30rpx;position:absolute;top:-22rpx;left:50%;margin-left:-15rpx}
.sp-button view.active{background:#CCAA77;width:190rpx;color:#CCAA77;text-align: left;background-image:url("data:image/png;base64,");background-size: 40rpx;background-repeat: no-repeat;background-position:center}
.sp-button view.act{background:#CCAA77;color:#fff}
.sp-add{border:2rpx dashed #C7C7C7;height:90rpx;margin:20rpx 50rpx;font-size:30rpx;line-height:80rpx;text-align:center;position:relative;margin-bottom:50rpx;}
.sp-add1{font-size:30rpx;color:#999}
.sp-add image{width:50rpx;height:50rpx;vertical-align:middle;margin-top:-4rpx;margin-right:10rpx}
.sp-add1 image{width:40rpx;height:40rpx}
.sp-buttotn{color:#CCAA77;border:2rpx solid #DDC7A5;border-radius:8rpx;position:fixed;bottom:18rpx;height:98rpx;line-height:100rpx;text-align:center;width:92%;background:#fff;left:4%}
.spec-status{background:#fff;font-size:28rpx;color:#333;height:100rpx;overflow-y:scroll;white-space:nowrap;padding-left:30rpx;padding-right:120rpx}
.spec-status view{display:inline-block;margin:0 20rpx;height:90rpx;line-height:90rpx}
.spec-status view.active{color:#CCAA77;border-bottom:4rpx solid #CCAA77}
.spec-class{color:#DDDDDD;font-size:30rpx}
.spec-input{border-bottom:2rpx solid #F6F6F6}
.spec-mes{background:rgba(0,0,0,0.4);color:#fff;font-size:24rpx;height:60rpx;line-height:60rpx;width:300rpx;text-align:center;border-radius:8rpx;top:-25rpx;position:absolute;left:50%;margin-left:-150rpx}
.spec-view{margin-top:50rpx;float:left;height:75rpx;line-height:75rpx;border:2rpx solid #BFBFBF;color:#999;width:260rpx;font-weight:normal;border-radius:8rpx;font-size:30rpx;padding-top:0 !important;text-align:center}
.spec-view-ac{background:#ddd;color:#fff;border:2rpx solid #ddd;margin-left:20rpx}
.spec-view-active{color:#fff;background:#CCAA77;border:2rpx solid #CCAA77}
swiper{height:85%}
.spec-title{font-size:46rpx;font-weight:bold;text-align:center;margin-bottom:80rpx;margin-top:40rpx}
swiper-item{overflow:scroll}
.spec-title-c{text-align: center}
