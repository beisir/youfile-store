                      <image src="/image/jian.png"></image>
                    </view>
                    <view class="lt-num" style='border-top:0'>
                      <input value='{{item.num}}' type='number' data-code='{{pItem.code}}' data-index='{{index}}' bindinput='changeNum'></input>
                    </view>
                    <view class="lt-add" data-index='{{index}}' bindtap="addCount1">
                      <image src="/image/lt-a.png"></image>
                    </view>
                  </view>
                </view>
              </view>
            </view>
          </view>
