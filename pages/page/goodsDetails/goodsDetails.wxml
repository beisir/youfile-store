          </view>
        </view>
        <view class='gd-gg lt-attr' wx:if='{{index==0}}'>
          <view class="lt-table">
            <view class="lt-tr">
              <view wx:if='{{!newSkuOnlyEdit}}' class='{{limitShow==1?"setCss":""}}'>{{nameTwo}}</view>
              <view style='{{newSkuOnly?"width:33%":"width:22%"}}' wx:if='{{limitShow!=1}}'>零售价</view>
              <view style='{{newSkuOnly?"width:50%":"width:22%"}}' wx:if='{{limitShow==1}}'>零售价</view>
