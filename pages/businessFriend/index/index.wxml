<view class='bf-cont'>
  <view class='bf-title'>批发商数据</view>
  <text>海量数据，最需要的数据信息</text>
  <view class='bt-view'>
    <view><view>{{merchantNumber}}</view>批发商总数</view>
    <view><view>{{waitPass}}</view>待验证</view>
    <view><view>{{waitVerify}}</view>等待通过</view>
  </view>
  <button class='bf-btn' bindtap='addWholesaler'>扫一扫 添加批发商</button>
  <view class='bf-weui'>
    <navigator class="weui_cells"  url='../mewWholesaler/mewWholesaler'>
      <image  mode="widthFix" src="/image/role.png"></image>
      <text >新增的批发商 </text>
      <view class='weui-view'>
        <image  mode="widthFix" src="/image/icl.png"></image>
      </view>
    </navigator>
    <navigator class="weui_cells" url='../wholesaler/wholesaler'>
      <image  mode="widthFix" src="/image/role.png"></image>
      <text>批发商管理 </text>
      <view class='weui-view'>
        <image  mode="widthFix" src="/image/icl.png"></image>
      </view>
    </navigator>
    <navigator class="weui_cells" url='../setUp/setUp'>
      <image  mode="widthFix" src="/image/role.png"></image>
      <text>商友权限设置 </text>
      <view class='weui-view'>
        <image  mode="widthFix" src="/image/icl.png"></image>
      </view>
    </navigator>
  </view>
</view>