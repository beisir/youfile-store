.box{padding: 20rpx;margin-bottom: 130rpx;}
.title-box{height: 120rpx; background: #fff;border-radius: 10rpx;padding:0 30rpx;}
.title-font{font-size: 34rpx;font-weight: 600;}
.img-num-text{font-size: 26rpx;color: #999;margin-left: 20rpx;}
.flex-between{display: flex;align-items: center;justify-content: space-between;}
.flex-center{display: flex;align-items: center;}
/* 模板 */
.img-box{ background: #fff;border-radius: 10rpx;margin-top: 20rpx;display: flex;flex-wrap: wrap;padding:30rpx 20rpx 10rpx;justify-content: space-around;}
.img-item-box{width: 209rpx;height: 446rpx;border: 1rpx solid #eee;border-radius: 10rpx;margin-bottom: 20rpx;position: relative;}
.img-module{width: 100%;height: 100%;}
.checked-icon{width: 35rpx;height: 35rpx;position: absolute;right: 0;top: 0;}
.img-box .empty-view{width: 209rpx;height: 446rpx;margin-bottom: 20rpx;}
/* 编辑 */
.title-input{border:1rpx solid #eee;border-radius: 10rpx;width: 413rpx;height: 78rpx;padding: 10rpx;box-sizing: border-box;font-size: 34rpx;font-weight: 600;}
.gold-color{color: #f8b616;}
.img-box .gold-border{border-color: #f8b616;}
/* 底部按钮 */
.bottom-btn-box{position: fixed;bottom: 0;left: 0;width: 100%;height: 120rpx;background: #fff;display: flex;justify-content: space-between;align-items: center;}
.bottom-btn-box button{background: #fff;color: #f8b616;font-size: 30rpx;border:0;}
.btn-line{width: 2rpx;height: 30rpx;background: #ddd;}

/* 弹框 */
.showmodal{padding-top:20rpx;}
.roomscroll{max-height: 500rpx;padding: 0 50rpx;box-sizing:border-box;}
.btn-group{display: flex;justify-content: space-between;}
.btn-group button{margin-top:50rpx;width:240rpx;height:70rpx;color:#fff;line-height:68rpx;border:2rpx solid #f8b616;font-size:30rpx;background:#f8b616;}
.btn-group .cancel-btn{color: #999;border: 1rpx solid #ddd;background: #fff;}

.room-checked-icon{width: 40rpx;height: 40rpx;margin-right: 40rpx;}
.room-item{height: 80rpx;font-size: 36rpx;}

.next-icon{width: 92rpx;height: 92rpx;position: fixed;right: 40rpx;bottom: 77rpx;}