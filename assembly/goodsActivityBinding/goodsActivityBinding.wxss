.active-g{height: 80rpx;position: absolute;bottom: 0;width: 94.4%;margin:0 2.8%;background-size: 100%;padding-top: 20rpx;background-repeat: no-repeat;background-image: url("data:image/png;base64,")}
.active-g>view{display: inline-block}
.active-g>view:last-child{width: 260rpx;height: 0;position: absolute;right: 0;top: -8rpx;color: #CCAA77;font-size: 26rpx;text-align:center;}
.active-g>view view{border-radius: 10rpx;margin-top:10rpx;padding-top: 12rpx;}
.active-g>view:first-child{font-size: 26rpx;color: #fff;padding-top: 10rpx;padding-left: 30rpx;}
.active-g>view:first-child text:first-child{font-size: 40rpx;}
.active-g>view:first-child text:last-child{text-decoration: line-through;margin-left: 10rpx;}